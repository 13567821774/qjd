\r\n        pc:true,\r\n        pa:false\r\n      },\r\n      pcolora:{\r\n        pc:false,\r\n        pa:true\r\n      },\r\n      isshowt:{\r\n        show:true,\r\n        hidden:false \r\n      },\r\n      isshowf:{\r\n        show:false,\r\n        hidden:true\r\n      }\r\n    }\r\n  },\r\n  methods:{\r\n    changeState(index){\r\n      // console.log(index,this.current[index].selected);\r\n      for(var i=0;i<this.current.length;i++){\r\n        if(i==index){\r\n          this.current[i].selected=true;\r\n        }else{\r\n          this.current[i].selected=false;\r\n        }\r\n      }\r\n    },\r\n    toPublish(){\r\n      this.$router.push('/Publish');\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n    *{\r\n      box-sizing: border-box;\r\n    }\r\n  .tabber{\r\n    padding:0;\r\n    margin: 0;\r\n    list-style: none;\r\n    display: flex;\r\n    /* justify-content: space-around; */\r\n    position: fixed;\r\n    bottom:0;\r\n    padding:.4rem .8rem .2rem;\r\n    height: 3.3rem;\r\n    background: #e9e9e9;\r\n    width: 100%;\r\n    z-index:98;\r\n  }\r\n  .tabber p{\r\n    margin: 0;\r\n    text-align: center;\r\n    font-size: .7rem;\r\n  }\r\n  .pc{color:#888;}\r\n  .pa{color:#000;}\r\n  .tabber li{\r\n    text-align: center;\r\n  }\r\n  .tabber>li:first-child{margin-right:3.2rem;}\r\n  .tabber>li:nth-child(2){margin-right:7.6rem;}\r\n  .tabber>li:nth-child(3){margin-right:3rem;}\r\n  /* .tabber>li:first-child{margin-left:.1rem;};\r\n  .tabber>li:nth-child(2){margin-left:0.5rem;margin-right:.5rem;} */\r\n  .tabber img{\r\n    width: 1.6rem;\r\n    display: block;\r\n    margin:0 auto;\r\n  }\r\n  .publish{\r\n    position: fixed;\r\n    bottom: 0.5rem;\r\n    width:3.6rem;\r\n    height:3.6rem;\r\n    line-height: 3.2rem;\r\n    z-index:99;\r\n    left:50%;\r\n    margin-left:-1.6rem;\r\n    background:#fff;\r\n    text-align: center;\r\n    border-radius:50%;\r\n    box-shadow: 0 -.025rem .025rem .025rem #ddd;\r\n\r\n  }\r\n  .publish>img{\